<Window x:Class="DMAM.Test.Controls.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dmamc="clr-namespace:DMAM.Controls;assembly=DMAM.Controls"
        Title="MainWindow" Height="350" Width="525" FontFamily="Tahoma" WindowStartupLocation="CenterScreen">
    <Grid>
        <dmamc:FieldSetEditor HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0"
                              Width="300" Height="250" Background="Beige"
                              FieldSet="{Binding FieldSet}" VerticalGapMargin="0" />
    </Grid>
</Window>
